{
    "manifest_version": 2,
    "name": "Azure DevOps Search",
    "description": "Quickly and easily search your Azure DevOps (formerly VSTS) project directly from this extension.",
    "version": "1.0.2",
    "author": "Eve Ragins",
    "options_page": "options.html",
    "browser_action": {
        "default_title": "Search Azure DevOps",
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon16.png",
            "32": "images/icon32.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }
    },
    "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "windows": "Alt+Shift+D"
            }
        }
    },
    "permissions": [
        "storage"
    ]
}